{
  "name": "prototype",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "test:angular": "ng test",
    "build:angular": "ng build",
    "build:sass": "sass --no-source-map src/sass:public/css",
    "copy:assets2": "copyfiles -u 1 ./dist/assets/**/* public",
    "copy:assets": "copyfiles -u 1 ./src/assets/**/* public",
    "copy:js": "copyfiles -u 1 ./dist/*.{js,js.map} public",
    "copy:css": "copyfiles -u 1 ./dist/*.{css,css.map} public",
    "copy:html": "copyfiles -u 1 ./dist/*.{html,ico} public",
    "copy:index": "copyfiles -u 1 ./src/*.{html,ico} public",
    "copy": "npm-run-all --parallel copy:*",
    "watch:assets2": "onchange \"dist/assets/**/*\" -- npm run copy:assets2",
    "watch:assets": "onchange \"src/assets/**/*\" -- npm run copy:assets",
    "watch:js": "onchange \"dist/*.{js,js.map}\" -- npm run copy:js",
    "watch:css": "onchange \"dist/*.{css,css.map}\" -- npm run copy:css",
    "watch:html": "onchange \"dist/*.{html,ico}\" -- npm run copy:html",
    "watch:index": "onchange \"src/*.{html,ico}\" -- npm run copy:index",
    "watch:sass": "sass --watch src/sass:public/css",
    "watch": "npm-run-all --parallel watch:*",
    "serve:static": "browser-sync start --server public --files public",
    "serve": "lite-server",
    "start:angular": "ng serve",
    "start": "npm-run-all copy --parallel watch serve",
    "build": "npm-run-all copy:html build:*",
    "prebuild": "ng build --watch --configuration development",
    "postbuild": "postcss public/css/*.css -u autoprefixer cssnano -r --no-map"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.2",
    "@angular/cdk": "^13.3.3",
    "@angular/common": "~13.3.2",
    "@angular/compiler": "~13.3.2",
    "@angular/core": "~13.3.2",
    "@angular/forms": "~13.3.2",
    "@angular/localize": "~13.3.2",
    "@angular/material": "^13.3.3",
    "@angular/platform-browser": "~13.3.2",
    "@angular/platform-browser-dynamic": "~13.3.2",
    "@angular/router": "~13.3.2",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.2",
    "@angular/cli": "^13.3.2",
    "@angular/compiler-cli": "~13.3.2",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "copyfiles": "^2.4.1",
    "cssnano": "^5.0.17",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "lite-server": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "postcss-cli": "^9.1.0",
    "typescript": "~4.5.2"
  }
}